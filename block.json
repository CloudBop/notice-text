{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "blox/notice-text",
	"title": "Notice Text",
	"category": "text",
	"description": "A box of text wrapped in borders, colours and styling.",
	"keywords": [ "text", "paragraph", "box" ],
	"supports": {
		"html": false
	},
	"styles": [
		{
			"name": "information",
			"label": "Information",
			"isDefault": true
		},
		{
			"name": "warning",
			"label": "Warning"
		},
		{
			"name": "error",
			"label": "Error"
		},
		{
			"name": "important",
			"label": "Important"
		}
	],
	"textdomain": "notice-text",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
